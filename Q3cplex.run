reset;
# Define the variables

var x1 >=0 , <=1 ;
var x2>=0 , <=1;
var x3>=0 , <=1;
var x4>=0 , <=1;


# Define the objective function
minimize z: x1 + x2 + x3 + x4;

# Define the constraints
s.t. c1: x1 +x2 >= 1;
s.t. c2: x2 + x4 >= 1;
s.t. c3: x2 + x3 >= 1;
s.t. c4: x3 + x4 >= 1;


# Set the solver options and solve the problem
option solver cplex;

solve;
display x1,x2 , x3 ,x4, z;